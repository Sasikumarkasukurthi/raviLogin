pipeline {
    agent any
    stages{
        stage('clone code') {
            steps{
            git branch: 'main', url: 'https://github.com/Sasikumarkasukurthi/Dockerproject.git'
            }
        }
        stage('build'){
            steps{
            sh 'docker build -t koti .'
            }
        }
    stage('runner') {
        steps {
           sh 'docker run --name kotiq -d -p98:80 koti:latest'
        }
    }
    }
}
